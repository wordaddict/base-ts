{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "quotemark": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "variable-name": false,
    "interface-name": false,
    "ordered-imports": false,
    "curly": false,
    "max-line-length": {
      "options": 180
    }
  },
  "rulesDirectory": []
}
